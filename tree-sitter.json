{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "spectec",
      "camelcase": "SpecTec",
      "title": "SpecTec",
      "scope": "source.spectec",
      "file-types": [
        "watsup",
        "spectec"
      ],
      "injection-regex": "^spectec$",
      "class-name": "TreeSitterSpectec"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Spectec grammar for tree-sitter",
    "authors": [
      {
        "name": "KunJeong",
        "email": "kunjeong99@naver.com"
      }
    ],
    "links": {
      "repository": "https://github.com/KunJeong/tree-sitter-spectec"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
